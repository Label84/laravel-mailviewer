{
    "name": "label84/laravel-mailviewer",
    "description": "A mail viewer package.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Label84",
            "email": "info@label84.com"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "illuminate/auth": "^7.0 || ^8.0",
        "illuminate/routing": "^7.0 || ^8.0",
        "illuminate/support": "^7.0 || ^8.0",
        "illuminate/database": "^7.0 || ^8.0"
    },    
    "autoload": {
        "psr-4": {
            "Label84\\MailViewer\\": "src",
            "Label84\\MailViewer\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Label84\\MailViewer\\Tests\\": "tests"
        }
    },    
    "extra": {
        "laravel": {
            "providers": [
                "Label84\\MailViewer\\MailViewerServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "nunomaduro/larastan": "^0.6.11",
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

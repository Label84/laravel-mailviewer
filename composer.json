{
    "name": "label84/laravel-mailviewer",
    "description": "View mails sent by Laravel in the browser",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Label84",
            "email": "info@label84.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/auth": "^11.0|^12.0",
        "illuminate/routing": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "larastan/larastan": "^3.1",
        "laravel/pint": "^1.21"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Label84\\MailViewer\\MailViewerServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Label84\\MailViewer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Label84\\MailViewer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

{
    "name": "label84/laravel-mailviewer",
    "description": "View mails sent by Laravel in the browser",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Label84",
            "email": "info@label84.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/auth": "^9.0|^10.0",
        "illuminate/routing": "^9.0|^10.0",
        "illuminate/support": "^9.0|^10.0",
        "illuminate/database": "^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0",
        "nunomaduro/larastan": "^2.0",
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Label84\\MailViewer\\MailViewerServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Label84\\MailViewer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Label84\\MailViewer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
